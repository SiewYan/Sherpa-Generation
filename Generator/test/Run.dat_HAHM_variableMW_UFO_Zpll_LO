(run){
  # general settings
  EVENTS 100;

  # me generator setup
  ME_SIGNAL_GENERATOR Comix;
  SCALES VAR{Abs2(p[0]+p[1])};
  
  # model setup
  MODEL HAHM_variableMW_UFO;

  # uncomment to disable underlying event simulation:
  # MI_HANDLER None;

  # uncomment to disable hadronization:
  # FRAGMENTATION Off;

  # uncomment to disable shower:
  # SHOWER_GENERATOR None;

  # LHC beam setup:
  BEAM_1 2212; BEAM_ENERGY_1 6500;
  BEAM_2 2212; BEAM_ENERGY_2 6500;

  # multi particlue definition
  PARTICLE_CONTAINER 9000 lepton 11 -11 13 -13;

}(run)

(processes){
  Process 93 93 -> 1023;
  Decay 1023 -> 9000 9000
  Order (*,2,*,*); 
  # Syntax: "Order (QCD,QED,HIG,HIW);"
  End process;
}(processes)

(selector){
  # phase space cuts for matrix elements
}(selector)

(ufo){
block yukawa
	4    1.42        # ymc
	5    4.7         # ymb
	6    174.3       # ymt
	15   1.777       # ymtau

